          <argument>
            <kind>1</kind>
            <string>//* Save Control *//
if(global.running)
    exit;

var file_name, file_id;

file_name = get_save_filename_ext("Life Files (*.lif)|*.lif", "", "", "Save");

if(file_name == "")
    exit;
    
if(string_pos(".lif", file_name) &lt;= 0)
    file_name += ".lif";

file_id = file_bin_open(file_name, 1);

show_debug_message("Binary File '" + string(file_id) + "' Opened for Write");

if(file_id == -1)
{
    show_debug_message("Attempting to Re-Open");
    file_id = file_bin_open(file_name, 1);

    if(file_id == -1)
    {
        window_set_caption("Game of Life | Error");
        show_message("Unable to Save to specified File");
        exit;
    }
}

file_bin_write_byte(file_id, instance_number(obj_cell));
file_bin_write_byte(file_id, view_xview);
file_bin_write_byte(file_id, view_yview);
file_bin_write_byte(file_id, view_wview);
file_bin_write_byte(file_id, view_hview);

with(obj_cell)
{
    file_bin_write_byte(file_id, x);
    file_bin_write_byte(file_id, y);
    file_bin_write_byte(file_id, floor(hue));
}

file_bin_close(file_id);
io_clear();
</string>
