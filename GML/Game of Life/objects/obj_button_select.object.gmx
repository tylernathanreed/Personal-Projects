        draw_sprite(spr_button, 1, x, y);
    }
    else if(hover)
        draw_sprite(spr_button, 0, x, y);

    if(fade &gt; 0.05)
    {
        fade -= 0.05;
        draw_set_alpha(fade);
        draw_sprite(spr_button, 1, x, y);
        draw_set_alpha(1);
    }

    if(!saving)
        draw_sprite(sprite_index, image_index, x, y);
    else if(saving)
        draw_sprite(sprite_index, image_index + 1, x, y);
}
else
    draw_sprite(sprite_index, disabled_index, x, y);

draw_set_font(font_state);
draw_set_color(c_dkgray);
