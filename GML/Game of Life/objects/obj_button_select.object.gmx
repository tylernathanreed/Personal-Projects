    pressed = hover &amp;&amp; mouse_check_button_pressed(mb_left);
    key_pressed = key &gt;= 0 &amp;&amp; keyboard_check_pressed(key);

    if(key_pressed)
    {
        // Secondary Trigger
        fade = 1;
        alarm[0] = 1;
