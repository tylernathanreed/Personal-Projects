          <argument>
            <kind>1</kind>
            <string>//* Draw Control *//
var i, j;

// Draw Background
draw_set_color(c_white);
draw_rectangle(0, 0, room_width, room_height, false);

// Draw Horizontal Lines
for(i = 0; i &lt; room_height; i += cell_size)
{
    draw_line_color(0, i, room_width/5, i, c_ltgray, c_white);
    draw_line_color(room_width*4/5, i, room_width, i, c_white, c_ltgray);
}

// Draw Vertical Lines
draw_set_color(c_ltgray);
for(i = 0; i &lt; room_width; i += cell_size)
{
    if(i &gt;= room_width/5 &amp;&amp; i &lt;= room_width*4/5)
        continue;

    if(i &lt; room_width/5)
        draw_set_alpha(1 - i/(room_width/5));
    else
        draw_set_alpha(1 - (room_width-i)/(room_width/5));

    draw_line(i, 0, i, room_height);
}

// Draw Leave Command
draw_set_color(c_black);
draw_set_font(font_state);
draw_text(view_xview, view_yview, "Press 'Esc' to Return");

// Draw Title
draw_set_halign(fa_center);
draw_set_font(font_title);
draw_text(room_width/2, 24, "Game of Life");

// Underline Title
draw_rectangle(room_width/2 - 128, 68, room_width/2 + 128, 72, false);

// Draw Subtitles
draw_set_font(font_subtitle);
draw_text(room_width/2, 80, "Concept: John Conway");
draw_text(room_width/2, 96, "Design: Boom");

// Draw Rules Header
draw_set_font(font_header);
draw_set_halign(fa_left);
draw_text(room_width/5, 200, "Rules:");

// Underline Header
draw_rectangle(room_width/5, 200+22, room_width/5 + string_width("Rules:"), 200+24, false);

// Draw Bullets
for(i = 0; i &lt; 5; i += 1)
    draw_circle(room_width/5 + 16, 242 + 18*i, 3, false);

// Draw Rules
draw_set_font(font_help);
draw_text(room_width/5 + 24, 232 + 18*0, "Cells are either Dead or Alive");

draw_text(room_width/5 + 24, 232 + 18*1, "Live Cells with Two or Three Neighbors live on to the next Generation");
draw_text(room_width/5 + 24, 232 + 18*2, "Live Cells with fewer than Two Neighbors die, as if by Under-population");
draw_text(room_width/5 + 24, 232 + 18*3, "Live Cells die with Four more or Neighbors, as if by Overcrowding");
draw_text(room_width/5 + 24, 232 + 18*4, "Dead Cells with exactly Three Neighbors become Alive, as if by Reproduction");

// Draw Controls Header
draw_set_font(font_header);
draw_text(room_width/5, 360, "Controls:");

// Underline Header
draw_rectangle(room_width/5, 360+22, room_width/5 + string_width("Controls:"), 360+24, false);

// Draw Bullets
for(i = 0; i &lt; 16; i += 1)
    draw_circle(room_width/5 + 16, 402 + 18*i, 3, false);

// Draw Icons
draw_cell(room_width/5+26, 396 + 18*0, 12, c_red); // Cell
draw_set_color(c_ltgray);
draw_rectangle(room_width/5+26, 396 + 18*1, room_width/5+26+12, 396 + 18*1 + 12, true);
draw_set_color(c_black);
draw_sprite(spr_button_clear, 0, room_width/5+20, 391 + 18*2); // New
draw_sprite(spr_button_load, 0, room_width/5+20, 391 + 18*3); // Load
draw_sprite(spr_button_save, 0, room_width/5+20, 391 + 18*4); // Save
draw_sprite(spr_button_run, 0, room_width/5+20, 391 + 18*5); // Play / Stop
draw_sprite(spr_button_next, 0, room_width/5+20, 391 + 18*6); // Next
draw_sprite(spr_button_shift, 0, room_width/5+20, 391 + 18*7); // Shift
draw_sprite(spr_button_color, 0, room_width/5+20, 391 + 18*8); // Color
draw_sprite(spr_button_speed, 0, room_width/5+20, 391 + 18*9); // Speed
draw_sprite(spr_button_hand, 0, room_width/5+20, 391 + 18*10); // Move
draw_sprite(spr_button_zoom_out, 0, room_width/5+20, 391 + 18*11); // Zoom Out
draw_sprite(spr_button_zoom_reset, 0, room_width/5+20, 391 + 18*12); // Zoom Reset
draw_sprite(spr_button_zoom_in, 0, room_width/5+20, 391 + 18*13); // Zoom Equals
draw_sprite(spr_button_grid, 0, room_width/5+20, 391 + 18*14); // Grid
draw_sprite(spr_button_help, 0, room_width/5+20, 391 + 18*15); // Help

// Draw Controls
draw_set_font(font_menu);
draw_text(room_width/5+46, 392 + 18*0, "New Cell                       - Revives the selected Cell");
draw_text(room_width/5+46, 392 + 18*1, "Delete Cell                          - Kills the selected Cell");
draw_text(room_width/5+46, 392 + 18*2, "New        - Clears the Grid");
draw_text(room_width/5+46, 392 + 18*3, "Load        - Loads a Save File");
draw_text(room_width/5+46, 392 + 18*4, "Save        - Saves a Save File");
draw_text(room_width/5+46, 392 + 18*5, "Play / Stop                         - Simulates the Game of Life");
draw_text(room_width/5+46, 392 + 18*6, "Next        - Simulates the next Generation");
draw_text(room_width/5+46, 392 + 18*7, "Shift        - Shifts all Cells in the given direction");
draw_text(room_width/5+46, 392 + 18*8, "Choose Color                  - Selects the Color of Cells that you place");
draw_text(room_width/5+46, 392 + 18*9, "Speed                          - Controls the Speed of Generations");
draw_text(room_width/5+46, 392 + 18*10, "Move        - Moves the View around the Grid");
draw_text(room_width/5+46, 392 + 18*11, "Zoom Out       - Zooms the View Out");
draw_text(room_width/5+46, 392 + 18*12, "Zoom Reset                - Resets the View Size");
draw_text(room_width/5+46, 392 + 18*13, "Zoom In       - Zooms the View In");
draw_text(room_width/5+46, 392 + 18*14, "Grid        - Toggles the Grid");
draw_text(room_width/5+46, 392 + 18*15, "Help           - Reveals the Help Menu");

// Draw Shortcuts
draw_set_font(font_shortcut)
draw_text(room_width/5+46 + string_width("New Cell "), 392 + 18*0, "(Left Click)");
draw_text(room_width/5+46 + string_width("Delete Cell "), 392 + 18*1, "(Right Click)");
draw_text(room_width/5+46 + string_width("New "), 392 + 18*2, "(N)");
draw_text(room_width/5+46 + string_width("Load "), 392 + 18*3, "(O)");
draw_text(room_width/5+46 + string_width("Save "), 392 + 18*4, "(S)");
draw_text(room_width/5+46 + string_width("Play / Stop "), 392 + 18*5, "(Spacebar)");
draw_text(room_width/5+46 + string_width("Next "), 392 + 18*6, "(X)");
draw_text(room_width/5+46 + string_width("Shift "), 392 + 18*7, "(F)");
draw_text(room_width/5+46 + string_width("Choose Color "), 392 + 18*8, "(Scroll)");
draw_text(room_width/5+46 + string_width("Speed "), 392 + 18*9, "(Up / Down)");
draw_text(room_width/5+46 + string_width("Move "), 392 + 18*10, "(H)");
draw_text(room_width/5+46 + string_width("Zoom Out "), 392 + 18*11, "(-)");
draw_text(room_width/5+46 + string_width("Zoom Reset "), 392 + 18*12, "(Enter)");
draw_text(room_width/5+46 + string_width("Zoom In "), 392 + 18*13, "(+)");
draw_text(room_width/5+46 + string_width("Grid "), 392 + 18*14, "(G)");
draw_text(room_width/5+46 + string_width("Help "), 392 + 18*15, "(F1)");

// Draw Save States Header
draw_set_font(font_header)
draw_text(room_width/5, 710, "Save States:");

// Underline Header
draw_rectangle(room_width/5, 710+22, room_width/5 + string_width("Save States:"), 710+24, false);

// Draw Save States Description
draw_set_font(font_help);
draw_text(room_width/5 + 16, 746 + 18*0, "Save States offer quick access to Saves.");
draw_text(room_width/5 + 16, 746 + 18*1, "You can either click on the Save State buttons,");
draw_text(room_width/5 + 16, 746 + 18*2, "or use the Number Keys to select them.");
draw_text(room_width/5 + 16, 746 + 18*3, "Standard selection will Load the Save State.");
draw_text(room_width/5 + 16, 746 + 18*4, "By clicking the 'Set' Button first, you can then");
draw_text(room_width/5 + 16, 746 + 18*5, "choose to save to a selected Save State.");
draw_text(room_width/5 + 16, 746 + 18*6, "Some Save States are provided for you by default.");
</string>
          </argument>
        </arguments>
      </action>
