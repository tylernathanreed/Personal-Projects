            <kind>1</kind>
            <string>//* Draw Control *//
event_inherited();

// Reset Mouse
if(!arrow &amp;&amp; window_get_cursor() == cr_none &amp;&amp; !obj_button_hand.hand)
{
    window_set_cursor(cr_default);
    cursor_sprite = -1;
}

// Disable Arrow
if(window_mouse_get_y() &lt; global.controller.border || mouse_check_button(mb_right))
    if(mouse_check_button_pressed(mb_any))
        arrow = false;

if(arrow)
{
    // Determine Cursor
    var mx, my, cx, cy, angle;
    mx = window_mouse_get_x();
    my = window_mouse_get_y();
    cx = window_get_width()/2;
    cy = window_get_height()/2;

    angle = point_direction(cx, cy, mx, my);
    index = round(angle / 45);

    if(index == 8)
        index = 0;

    // Draw Cursor
    if(mouse_check_button(mb_left))
        draw_sprite_ext(spr_button_shift, index, mx - 15, my - 15, 1.25, 1.25, 0, c_ltgray, 1);
    else
        draw_sprite_ext(spr_button_shift, index, mx - 18, my - 18, 1.5, 1.5, 0, c_white, 1);

    // Shift Cells
    if(mouse_check_button_pressed(mb_left))
    {
        // Clear Grids
        with(obj_cell)
        {
            ds_grid_set(global.colors, xx, yy, 0);
            ds_grid_set(global.cells, xx, yy, 0);
        }

        // Recallibrate Grids
        with(obj_cell)
        {
            // Shift Left/Right
            if(other.index == 0 || other.index == 1 || other.index == 7)
                x += cell_size;
            else if(other.index == 3 || other.index == 4 || other.index == 5)
                x -= cell_size;

            // Shift Up/Down
            if(other.index == 5 || other.index == 6 || other.index == 7)
                y += cell_size;
            else if(other.index == 1 || other.index == 2 || other.index == 3)
                y -= cell_size;

            xx = x div global.size; // New Grid X Index
            yy = y div global.size; // New Grid Y Index

            ds_grid_set(global.colors, xx, yy, make_color_hue(hue, 1));
            ds_grid_set(global.cells, xx, yy, 1);
        }
    }
}
</string>
